<table class="addresses-table">
  <tr>
    <th>Address</th>
    <th>Date Created</th>
  </tr>
  <% @btcaddresses.each do |btcaddress| %>
    <tr id="btcaddress">
      <td><%= btcaddress.address %></td>
      <td><%= btcaddress.created_at %></td>
    </tr>
  <% end %>
</table>
<%= paginate @btcaddresses %>
<br>
<%= link_to("Create New Address", btcaddresses_path, method: :post) %>
