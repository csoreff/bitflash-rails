<table>
  <tr>
    <th>Address</th>
    <th>Date Created</th>
  </tr>
  <% @addresses.each do |address| %>
    <tr>
      <td><%= address.btc_address %></td>
      <td><%= address.created_at %></td>
    </tr>
  <% end %>
</table>
<br>
<%= link_to("Create New Address", addresses_path, method: :post) %>
